.null {
  display: flex;
  align-items: center;
  flex-direction: column;
  margin-top: 300rpx;
}
.null .null-img {
  width: 400rpx;
}
.null .null-desc {
  margin-top: 30rpx;
  font-size: 30rpx;
  color: #999999;
}

.input-item {
  box-sizing: border-box;
  margin-top: 46rpx;
  display: flex;
}
.input-item .item-title {
  line-height: 100rpx;
  font-size: 32rpx;
  padding: 0 20rpx;
  width: 200rpx;
  height: 30rpx;
  text-align: justify;
  text-align-last: justify;
}
.input-item .item-title::after {
  content: "";
  display: inline-block;
  width: 100%;
}
.input-item .jinggao {
  position: absolute;
  right: -80rpx;
  top: 0;
  bottom: 0;
  margin: auto;
  font-size: 50rpx;
  color: #FF0202;
  height: 60rpx;
  width: 60rpx;
}
.input-item .item-xxx {
  display: flex;
}
.input-item .item-xxx .get-code {
  width: 180rpx;
  background-color: #1E5093;
  color: #ffffff;
  text-align: center;
  line-height: 94rpx;
  height: 94rpx;
  margin-left: 20rpx;
}
.input-item .item-xxx .button {
  background-color: #1E5093;
  line-height: 93rpx;
  height: 93rpx;
  color: #ffffff;
  font-size: 26rpx;
  border-radius: 0;
  text-align: center;
  width: 180rpx;
  padding: 0 !important;
}
.input-item .item-xxx .nobutton {
  background-color: #999999;
  line-height: 93rpx;
  height: 93rpx;
  color: #ffffff;
  font-size: 26rpx;
  border-radius: 0;
  text-align: center;
  width: 180rpx;
  padding: 0 !important;
}
.input-item .inner-item {
  padding-left: 5rpx;
  border: 1px solid #cccccc;
  border-top: 1px solid #000;
  display: flex;
  height: 90rpx;
  position: relative;
  margin-right: 20rpx;
}
.input-item .inner-item .line {
  background-color: #999999;
  width: 1rpx;
  height: 45rpx;
  margin-top: 25rpx;
  margin-right: 15rpx;
  margin-left: -5rpx;
}
.input-item .inner-item .item-icon {
  padding-right: 10rpx;
  padding-top: 14rpx;
}
.input-item .inner-item .item-icon .icon {
  font-size: 60rpx;
  color: #999999;
}
.input-item .inner-item .icon-car {
  font-size: 45rpx !important;
  line-height: 68rpx !important;
  padding-top: 3rpx !important;
}
.input-item .inner-item .input-mobile.code {
  width: 174rpx !important;
}
.input-item .inner-item .input-mobile {
  padding-top: 25rpx;
  width: 370rpx;
}
.input-item .inner-item .input-mobile input {
  line-height: 100rpx;
}
.input-item .inner-item .input-placeholder {
  color: #999999;
  font-size: 28rpx;
}
.input-item .pis {
  color: #666666;
  font-size: 24rpx;
}
.input-item .pis .inner-pis {
  display: flex;
  justify-content: space-between;
}
.input-item .pis .inner-pis .pis-speck {
  margin-top: 10rpx;
}
.input-item .pis .inner-pis .up-button {
  border: 1px solid #cccccc;
  padding: 20rpx 40rpx;
  margin-top: 20rpx;
  color: #999999;
}

.post {
  width: 550rpx;
  height: 90rpx;
  line-height: 90rpx;
  text-align: center;
  color: #ffffff;
  margin: 130rpx auto 0;
  background-color: #1E5093;
}
