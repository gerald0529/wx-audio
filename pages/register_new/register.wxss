.null {
  display: flex;
  align-items: center;
  flex-direction: column;
  margin-top: 300rpx;
}
.null .null-img {
  width: 400rpx;
}
.null .null-desc {
  margin-top: 30rpx;
  font-size: 30rpx;
  color: #999999;
}

page {
  display: flex;
  align-items: center;
  justify-content: center;
  height: 100%;
  background-image: url(data:image/jpeg;base64,);
  background-size: 100% 100%;
}

.register {
  width: 600rpx;
  padding: 50rpx;
  border-radius: 30rpx;
  background: #fff;
}
.register .box {
  display: flex;
  align-items: center;
  margin-bottom: 70rpx;
}
.register .box .icon {
  flex: none;
  font-size: 60rpx;
  color: #999;
}
.register .box .input {
  flex: 1;
  border-bottom: 1rpx solid rgba(204, 204, 204, 0.8);
}
.register .box .code {
  display: flex;
}
.register .box .code input {
  flex: 1;
}
.register .box .code .v-code {
  flex: none;
  color: #1b5999;
}
.register .agree {
  display: flex;
  align-items: center;
  width: 550rpx;
  margin: 100rpx auto 0;
  font-size: 28rpx;
  text-align: center;
}
.register .agree .a {
  font-size: 28rpx;
  margin-top: 5rpx;
}
.register .agree .b {
  color: #436DC9;
}
.register .post {
  width: 550rpx;
  height: 90rpx;
  line-height: 90rpx;
  border-radius: 90rpx;
  text-align: center;
  color: #ffffff;
  margin: 30rpx auto;
  background-color: #046ed1;
}
.register .login-box {
  text-align: right;
}
.register .login-box .login {
  display: inline;
  padding: 20rpx;
  color: #1a5898;
}

checkbox .wx-checkbox-input {
  width: 40rpx;
  height: 40rpx;
  border-radius: 50%;
}

checkbox .wx-checkbox-input.wx-checkbox-input-checked::before {
  color: #1b5999;
}
